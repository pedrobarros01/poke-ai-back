{
    "builds": [
        {
            "src": "src/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.py"
        }
    ],
    "functions": {
        "src/main.py": {
            "maxDuration": 50
        }
    }
}